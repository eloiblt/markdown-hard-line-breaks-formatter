// Place your settings in this file to overwrite default and user settings.
{
	"files.exclude": {
		"dist-web": false, // set this to true to hide the "dist" folder with the compiled JS files
		"dist-desktop": false // set this to true to hide the "dist" folder with the compiled JS files
	},
	"search.exclude": {
		"dist-desktop": true, // set this to false to include "dist" folder in search results
		"dist-web": true // set this to false to include "dist" folder in search results
	},
	// Turn off tsc task auto detection since we have the necessary tasks as npm scripts
	"typescript.tsc.autoDetect": "off"
}